<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
              android:orientation="vertical"
             tools:context="com.packtpub.apps.rxjava_essentials.example1.CreateFragment">

    <Button
        android:id="@+id/first"
        android:textAllCaps="false"
        android:onClick="first"
        android:text="first"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/firstFun"
        android:textAllCaps="false"
        android:onClick="firstFun"
        android:text="distinctFun"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/single"
        android:textAllCaps="false"
        android:onClick="single"
        android:text="single"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/elementAt"
        android:textAllCaps="false"
        android:onClick="elementAt"
        android:text="elementAt"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/sample"
        android:textAllCaps="false"
        android:onClick="sample"
        android:text="sample"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/throttleFirst"
        android:textAllCaps="false"
        android:onClick="throttleFirst"
        android:text="throttleFirst"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/debounce"
        android:textAllCaps="false"
        android:onClick="debounce"
        android:text="debounce"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/timeout"
        android:textAllCaps="false"
        android:onClick="timeout"
        android:text="timeout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/skip"
        android:textAllCaps="false"
        android:onClick="skip"
        android:text="skip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <Button
        android:id="@+id/unsubscribe"
        android:textAllCaps="false"
        android:onClick="unsubscribe"
        android:text="unsubscribe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <android.support.v4.widget.SwipeRefreshLayout
            android:id="@+id/fragment_first_example_swipe_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <android.support.v7.widget.RecyclerView
                android:id="@+id/fragment_first_example_list"
                android:clipToPadding="false"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingRight="@dimen/activity_horizontal_margin"
                android:paddingLeft="@dimen/activity_horizontal_margin"
                android:paddingTop="@dimen/activity_horizontal_margin"
                android:paddingBottom="@dimen/activity_vertical_margin"/>
    </android.support.v4.widget.SwipeRefreshLayout>

</LinearLayout>
